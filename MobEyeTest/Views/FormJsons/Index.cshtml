@model IEnumerable<MobEyeTest.Models.FormJson>

@{
    ViewData["Title"] = "Index";
}



<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("List")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormContent)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit Form</a> |
                <a asp-action="Details" asp-route-id="@item.Id">View Form</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete Form</a>
            </td>
        </tr>
}
    </tbody>
</table>
